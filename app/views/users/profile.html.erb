<div class="row-fluid">
	<div class="span3">
		<%= render_panel(current_user.role) %>
	</div>

	<div class="panel-sign-in span9"  style="margin-top: 40px;">
		<div class="panel-heading-sign-in">
			<h4>Dashboard</h4>
		</div>
		<div class="panel-body-sign-in">
			<% if current_user.logo.present? %>
				<%= image_tag(current_user.logo.logo.url(:medium))%>
			<% else %>
				<%= image_tag("missing.jpg")%>
			<% end %>
			<%= render_profile(current_user.role) %>
		</div>
	</div>
</div>